{
  "name": "optimistdigital/nova-simple-repeatable",
  "description": "A Laravel Nova simple repeatable rows field.",
  "keywords": [
    "laravel",
    "nova",
    "repeatable",
    "flexible",
    "rows",
    "keyvalue",
    "field"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.2.0",
    "laravel/nova": "^3.0|^4.0",
    "optimistdigital/nova-translations-loader": "*"
  },
  "autoload": {
    "psr-4": {
      "OptimistDigital\\NovaSimpleRepeatable\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "OptimistDigital\\NovaSimpleRepeatable\\SimpleRepeatableServiceProvider"
      ]
    }
  },
  "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/bsormagec/nova-translations-loader"
        }
    ],
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
